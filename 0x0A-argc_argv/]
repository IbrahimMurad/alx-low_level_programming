#include <stdio.h>

/**
 * main - Entry point
 * @argc: the number of arguments passed to main function
 * @argv: the array that includes the arguments as strings
 *
 * Return: void
 */

int main(int argc, char *argv[])
{
	int i;

	if (argc != 1)
	{
		return (0);
	}

	for (i = 0; argv[0][i] != '\0'; i++)
	{
		printf("%c", argv[0][i]);
	}
	printf("\n");
	return (0);
}
